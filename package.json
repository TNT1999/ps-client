{
  "name": "kltn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "next": "next dev -p 8080",
    "dev": "run-p next watch:css",
    "build": "yarn run build:css && next build",
    "start": "next start -p 8080",
    "lint": "eslint . --ext=js,jsx,ts,tsx --ignore-path .gitignore --ignore-pattern node_modules/",
    "lint:fix": "./node_modules/.bin/eslint . --ext=js,jsx,ts,tsx --fix --ignore-path .gitignore --ignore-pattern node_modules/",
    "prepare": "husky install",
    "check:pret": "prettier --check .",
    "format": "prettier --write .",
    "pre-commit": "lint-staged",
    "optimize-svg": "svgo -r -f public/assets",
    "watch:css": "postcss -w ./styles/tailwind.css -o ./styles/main.css",
    "build:css": "postcss ./styles/tailwind.css -o ./styles/main.css"
  },
  "dependencies": {
    "@headlessui/react": "^1.2.0",
    "@heroicons/react": "^1.0.1",
    "@nextui-org/react": "^1.0.2-alpha.2",
    "@reduxjs/toolkit": "^1.6.0",
    "@tippyjs/react": "^4.2.6",
    "@udecode/plate": "^10.6.0",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.0",
    "framer-motion": "^6.3.0",
    "lodash": "^4.17.21",
    "next": "11",
    "next-images": "^1.8.4",
    "nextjs-progressbar": "^0.0.11",
    "nookies": "^2.5.2",
    "qs": "^6.10.3",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hint": "^3.2.1",
    "react-hook-form": "^7.8.8",
    "react-redux": "^7.2.4",
    "react-smooth-dnd": "^0.11.1",
    "react-toastify": "^8.2.0",
    "react-transition-group": "^4.4.2",
    "react-use": "^17.3.1",
    "redux-logger": "^3.0.6",
    "sha256": "^0.2.0",
    "slate": "^0.76.1",
    "slate-history": "^0.66.0",
    "slate-hyperscript": "^0.67.0",
    "slate-react": "^0.76.1",
    "slate-rte": "^1.0.85",
    "styled-components": "^5.3.5",
    "swiper": "^8.0.7",
    "swr": "^0.5.6",
    "use-async-effect": "^2.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.3",
    "@commitlint/config-conventional": "^16.0.0",
    "@next/eslint-plugin-next": "^12.0.8",
    "@svgr/webpack": "^6.2.1",
    "@types/lodash": "^4.14.170",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@types/react": "17.0.9",
    "@types/react-hint": "^3.2.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-select": "^4.0.17",
    "@types/redux-logger": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.2.6",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cssnano": "^5.1.7",
    "eslint": "^7.30.0",
    "eslint-config-next": "^12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.2.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.0",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.1.0",
    "prettier": "^2.3.2",
    "svgo": "^2.8.0",
    "tailwindcss": "^3.0.23",
    "typescript": "4.3.2",
    "wait-on": "^6.0.1"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js}": [
      "yarn run lint:fix",
      "yarn run format"
    ]
  }
}