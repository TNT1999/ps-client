{
  "name": "kltn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8080",
    "build": "next build",
    "start": "next start -p 8080",
    "lint": "eslint . --ext=js,jsx,ts,tsx --ignore-path .gitignore --ignore-pattern node_modules/",
    "lint:fix": "eslint . --ext=js,jsx,ts,tsx --fix --ignore-path .gitignore --ignore-pattern node_modules/",
    "prepare": "husky install",
    "check:pret": "prettier --check .",
    "format": "prettier --write .",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@headlessui/react": "^1.2.0",
    "@heroicons/react": "^1.0.1",
    "@nextui-org/react": "^1.0.2-alpha.2",
    "@reduxjs/toolkit": "^1.6.0",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "next": "10.2.3",
    "next-images": "^1.8.4",
    "nextjs-progressbar": "^0.0.11",
    "query-string": "^7.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.8.8",
    "react-redux": "^7.2.4",
    "react-use": "^17.3.1",
    "redux-logger": "^3.0.6",
    "swr": "^0.5.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.3",
    "@commitlint/config-conventional": "^16.0.0",
    "@next/eslint-plugin-next": "^12.0.8",
    "@tailwindcss/custom-forms": "^0.2.1",
    "@types/lodash": "^4.14.170",
    "@types/nprogress": "^0.2.0",
    "@types/react": "17.0.9",
    "@types/react-redux": "^7.1.16",
    "@types/react-select": "^4.0.17",
    "@types/redux-logger": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.2.6",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^7.30.0",
    "eslint-config-next": "^12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.2.1",
    "postcss": "^8.3.0",
    "prettier": "^2.3.2",
    "tailwindcss": "^2.1.4",
    "typescript": "4.3.2"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js}": [
      "yarn run lint:fix",
      "yarn run format"
    ]
  }
}
